{
  "extends": "expo-module-scripts/tsconfig.plugin",
  "compilerOptions": {
    "outDir": "./build",
    "rootDir": "./",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "allowArbitraryExtensions": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["build/**/*", "node_modules/**/*", "**/__mocks__/*", "**/__tests__/*", "**/*.d.ts"]
}