{
  "expo": {
    "name": "NitroScreenRecorderExample",
    "slug": "NitroScreenRecorderExample",
    "scheme": "nitroscreenrecorderexample",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "nitroscreenrecorderexample.example",
    },
    "android": {
      "package": "nitroscreenrecorderexample.example"
    },
    "plugins": [
      "expo-video",
      [
        "../app.plugin.js",
        {
          "cameraPermissionText": "ScreenRecorder needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "ScreenRecorder needs access to your Microphone.",
          "showPluginLogs": true
      }
      ]
    ]
  }
}
